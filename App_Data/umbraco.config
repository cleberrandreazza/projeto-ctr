      <pistasItem id="1175" key="505ce916-f669-47a4-ad74-c54e8220546c" parentID="1146" level="3" creatorID="0" sortOrder="0" createDate="2017-07-20T11:10:24" updateDate="2017-08-21T14:55:58" nodeName="01 - Estrada de Chão" urlName="01-estrada-de-chão" path="-1,1070,1146,1175" isDoc="" nodeType="1172" creatorName="Felipe Gubert" writerName="Wagner Dias" writerID="1" template="1171" nodeTypeAlias="pistasItem">
